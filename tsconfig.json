    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es6",
    "baseUrl": "./",
    "types": [
      "jest",
      "node"
    ],
    "paths": {
       "src/*":["./src/*"]
     }
  },
  "include": [
      "./src/**/*"
  ]
